{
  "manifest_version": 2,
  "name": "resume_autofill",
  "version": "1",
  "permissions": [
	"tabs",
	"storage"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action":{
	"default_popup": "raf.html",
    "default_icon": {
        "48": "icons/rafLogo.svg"
    }
  },

  "description": "helps with those annoying resume application sites",

  "icons": {
    "48": "icons/rafLogo.svg"
  },

  "content_scripts": [
    {
	"matches": ["<all_urls>"],
        "js": ["raf.js"]
    }
  ]
}

